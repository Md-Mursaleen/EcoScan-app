{
  "expo": {
    "name": "EcoScan",
    "slug": "ecoscanapp",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "assets": [
      "./assets/fonts/"
    ],
    "scheme": "ecoscan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "backgroundColor": "#FFFFFF",
          "resizeMode": "contain"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "EcoScan needs access to your photos to allow you to upload clothing images to analyze and calculate their carbon footprint."
        }
      ],
      "expo-web-browser"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecoscan.app"
    },
    "android": {
      "package": "com.ecoscan.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "versionCode": 1,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "4ef71b89-ffac-4c00-93d3-81701a5805d1"
      }
    },
    "owner": "md_mursaleen"
  }
}